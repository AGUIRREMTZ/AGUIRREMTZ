#include <stdio.h>

int main() {
    // Declaración e inicialización de una matriz 3x3
    int matriz[3][3] = {
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
    };

    // Acceso e impresión de elementos de la matriz
    printf("Elemento en la fila 2, columna 3: %d\n", matriz[1][2]);

    // Modificación de un elemento de la matriz
    matriz[0][1] = 10;

    // Imprimir la matriz después de la modificación
    printf("Matriz después de la modificación:\n");
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            printf("%d ", matriz[i][j]);
        }
        printf("\n");
    }

    return 0;
}
